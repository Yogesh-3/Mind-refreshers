C = ['b','c','d','f','g','h','j','k','l','m','n','p','q','r','s','t','v','w','x','y','z','B','C','D','F','G','H','J','K','L','M','N','P','Q','R','S','T','V','W','X','Y','Z']
V= ['a','e','i','o','u','A','E','I','O','U']

ch = str(input())               #input character
if (ch in C):
    print ("Consonant")
elif (ch in V):
    print ("Vowel")
else:
    print("invalid")
